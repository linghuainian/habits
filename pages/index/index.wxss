.card{
  margin: 10px;
  position: relative;
}
.bg{
  border-radius: 5px;
  width: 100%;
  box-shadow:0 0 5px #5F687E;
}
.logo{
  width: 50px;
  position: absolute;
  top:10px;
  right: 20px;
  
}
.info{
  position: absolute;
  left: 30px;
  bottom: 30px;
  font-size: 14px;
  color: #5F687E;

}

.name{
  font-weight: bold;
}

.desc{
  margin-bottom: 10px;
}



